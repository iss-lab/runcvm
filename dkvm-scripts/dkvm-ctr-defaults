DKVM=/opt/dkvm
DKVM_PATH=$DKVM/usr/sbin:$DKVM/usr/bin:$DKVM/sbin:$DKVM/bin:$DKVM/usr/lib/qemu

QEMU_IFUP="$DKVM/scripts/dkvm-ctr-qemu-ifup"
QEMU_IFDOWN="$DKVM/scripts/dkvm-ctr-qemu-ifdown"
QEMU_BRIDGE='q0'

QEMU_VIRTIOFSD_SOCKET=/run/.virtiofs.sock
QEMU_GUEST_AGENT=/run/.qemu-guest-agent
QEMU_MONITOR_SOCKET=/run/.qemu-monitor-socket